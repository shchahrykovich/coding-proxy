{
  "name": "coding-proxy",
  "version": "0.3.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage ./tests/app ./tests/crons",
    "db:migrations:apply": "wrangler d1 migrations apply DB --remote",
    "deploy-from-local": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy": "npm run db:migrations:apply && npx opennextjs-cloudflare build && wrangler deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.1.0",
    "@ant-design/plots": "^2.6.3",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@anthropic-ai/sdk": "^0.60.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@opennextjs/cloudflare": "^1.6.5",
    "@prisma/adapter-d1": "^6.15.0",
    "@prisma/client": "^6.15.0",
    "antd": "^5.27.1",
    "bcryptjs": "^3.0.2",
    "lodash": "^4.17.21",
    "next": "15.5.2",
    "next-auth": "^5.0.0-beta.29",
    "ohash": "^2.0.11",
    "openai": "^5.12.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.70",
    "@eslint/eslintrc": "^3",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.15.0",
    "typescript": "^5",
    "vitest": "~3.2.0",
    "wrangler": "^4.34.0"
  },
  "cloudflare": {
    "bindings": {
      "AUTH_SECRET": {
        "description": "Secret for auth. `openssl rand -hex 32` - use to generate on mac."
      },
      "ANTHROPIC_API_KEY": {
        "description": "Anthropic API Key."
      },
      "ANTHROPIC_BASE_URL": {
        "description": "[Optional] Anthropic Base Url. Optional parameter. Useful for caching and saving costs."
      },
      "CLOUDFLARE_AI_GATEWAY_TOKEN": {
        "description": "[Optional] Cloudflare Token for AI Gateway. Optional parameter. Useful for caching and saving costs."
      }
    }
  }
}
